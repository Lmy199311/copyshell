#!/bin/sh
#批量行权容量测试 百万持仓，十万投资者 
for i in {1..100000};do j=$((130000 + $i));
echo \"${j}\",\"8000\",\"\",\"Name${j}\",\"1\",\"Card${j}\",\"1\",\"20617759\",\"\",\"20140101\",\"20617759\",\"\",\"\",\"\" >> t_Investor.csv
done
for a in {1..100000};do j=$((130000 + $a));k=$((4172+$a));
echo \"SR611\",\"8000\",\"${j}\",\"1\",\"0\",\"20140101\",\"${k}\",\"1\",\"300\",\"20160926\",\"1\",\"0\",\"\",\"CZCE\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"300\",\"0\",\"0\",\"0\" >>~/tinit/perf/t_InvestorPositionDtl.csv
done
for b in {1..100000};do j=$((130000 + $b));h=$(($k+$b));
echo \"SR701\",\"8000\",\"${j}\",\"1\",\"1\",\"20140101\",\"${h}\",\"1\",\"300\",\"20160926\",\"1\",\"0\",\"\",\"CZCE\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"300\",\"0\",\"0\",\"0\" >> ~/tinit/perf/t_InvestorPositionDtl.csv
done
for c in {1..100000};do j=$((130000 + $c)); q=$(($h+$c));
echo \"rb1212\",\"8000\",\"${j}\",\"1\",\"0\",\"20111216\",\"${q}\",\"1\",\"5800\",\"20111118\",\"1\",\"0\",\"\",\"SHFE\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"5700\",\"0\",\"0\",\"0\" >> ~/tinit/perf/t_InvestorPositionDtl.csv
done
for d in {1..100000};do j=$((130000 + $d)); p=$(($q+$d));
echo \"rb1302\",\"8000\",\"${j}\",\"1\",\"0\",\"20111216\",\"${p}\",\"1\",\"5800\",\"20111118\",\"1\",\"0\",\"\",\"SHFE\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"5700\",\"0\",\"0\",\"0\"  >> ~/tinit/perf/t_InvestorPositionDtl.csv
done
for e in  {1..100000};do j=$((130000 + $e)); f=$(($p+$e));
echo \"a1601\",\"8000\",\"${j}\",\"1\",\"1\",\"20140715\",\"${f}\",\"1\",\"5800\",\"20140815\",\"1\",\"0\",\"\",\"DCE\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"5700\",\"0\",\"0\",\"0\" >> ~/tinit/perf/t_InvestorPositionDtl.csv
done
for x in {1..100000};do j=$((130000 + $x)); m=$(($f+$x));
echo \"a1601-C-3900\",\"8000\",\"${j}\",\"1\",\"0\",\"20111216\",\"${m}\",\"3\",\"5800\",\"20111118\",\"1\",\"0\",\"\",\"DCE\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"5700\",\"0\",\"0\",\"0\" >> ~/tinit/perf/t_InvestorPositionDtl.csv
done
for y in {1..100000};do j=$((130000 + $y)); n=$(($m+$y));
echo \"c64000rb1212\",\"8000\",\"${j}\",\"1\",\"0\",\"20111216\",\"${n}\",\"3\",\"5800\",\"20111118\",\"1\",\"0\",\"\",\"SHFE\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"0\",\"5700\",\"0\",\"0\",\"0\" >> ~/tinit/perf/t_InvestorPositionDtl.csv
done
